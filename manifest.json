{
    "manifest_version" : 3,
    "name": "focusTimer",
    "version" : "1.0",
    "description": "helps you to focus",
    "icons" :{
        "16" : "time.png",
        "48" : "time.png",
        "128" : "time.png"
    },
    "action":{
    "default_icons": "time.png",
    "default_name": "FocusTimer",
    "default_popup":"popup/popup.html"
    },
    "permissions" : ["storage","alarms","notifications"],

    "background" : {
        "service_worker":"background.js"
    },
    "options_page" : "options/options.html"
}